            </trans-unit>
            <trans-unit id="content.status.scheduled" resname="content.status.scheduled">
                <source>Scheduled</source>
            </trans-unit>
            <trans-unit id="content.status.expired" resname="content.status.expired">
                <source>Expired</source>
            </trans-unit>
