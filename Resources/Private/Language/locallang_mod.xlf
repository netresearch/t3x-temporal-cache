            <trans-unit id="wizard.title" resname="wizard.title">
                <source>Configuration Wizard</source>
            </trans-unit>
            <trans-unit id="wizard.back" resname="wizard.back">
                <source>Back</source>
            </trans-unit>

            <!-- Wizard: Welcome -->
            <trans-unit id="wizard.welcome.title" resname="wizard.welcome.title">
                <source>Welcome to Temporal Cache Configuration Wizard</source>
            </trans-unit>
            <trans-unit id="wizard.welcome.description" resname="wizard.welcome.description">
                <source>This wizard will help you configure the Temporal Cache extension for optimal performance based on your site's temporal content patterns.</source>
            </trans-unit>
            <trans-unit id="wizard.welcome.current_stats" resname="wizard.welcome.current_stats">
                <source>Your Current Statistics:</source>
            </trans-unit>
            <trans-unit id="wizard.welcome.stat.total" resname="wizard.welcome.stat.total">
                <source>%d temporal content elements</source>
            </trans-unit>
            <trans-unit id="wizard.welcome.stat.transitions" resname="wizard.welcome.stat.transitions">
                <source>%d average transitions per day</source>
            </trans-unit>
            <trans-unit id="wizard.welcome.stat.harmonizable" resname="wizard.welcome.stat.harmonizable">
                <source>%d elements can be harmonized</source>
            </trans-unit>
            <trans-unit id="wizard.welcome.start" resname="wizard.welcome.start">
                <source>Start Configuration</source>
            </trans-unit>

            <!-- Wizard: Analysis -->
            <trans-unit id="wizard.analysis.title" resname="wizard.analysis.title">
                <source>Configuration Analysis</source>
            </trans-unit>
            <trans-unit id="wizard.analysis.description" resname="wizard.analysis.description">
                <source>Based on your site's temporal content patterns, here are our recommendations:</source>
            </trans-unit>
            <trans-unit id="wizard.analysis.recommendations" resname="wizard.analysis.recommendations">
                <source>Recommendations</source>
            </trans-unit>
            <trans-unit id="wizard.analysis.continue" resname="wizard.analysis.continue">
