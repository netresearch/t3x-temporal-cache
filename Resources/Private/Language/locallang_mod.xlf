            <!-- Content List -->
            <trans-unit id="content.title" resname="content.title">
                <source>Temporal Content Management</source>
            </trans-unit>
            <trans-unit id="content.filter.all" resname="content.filter.all">
                <source>All Content</source>
            </trans-unit>
            <trans-unit id="content.filter.pages" resname="content.filter.pages">
